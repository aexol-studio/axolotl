{
  "name": "new",
  "version": "2.1.9",
  "description": "Automatically generated by graphql-editor-cli",
  "main": "lib/index.js",
  "type": "module",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "tsx src/prisma/seed.ts"
  },
  "scripts": {
    "start": "NODE_ENV=production node lib/index.js",
    "build": "tspc && npm run build:client && npm run build:server",
    "build:backend": "tspc",
    "build:client": "vite build",
    "build:server": "vite build --ssr src/entry-server.tsx",
    "watch": "tspc --watch",
    "models": "axolotl build",
    "resolvers": "axolotl resolvers",
    "inspect": "axolotl inspect -s schema.graphql -r ./lib/resolvers.js",
    "chaos": "axolotl chaos -s schema.graphql -u http://localhost:4000/graphql",
    "dev": "nodemon",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "postinstall": "prisma generate --schema src/prisma/schema.prisma"
  },
  "author": "GraphQL Editor Centaur Generator",
  "license": "ISC",
  "dependencies": {
    "@aexol/axolotl-core": "^2.1.9",
    "@aexol/axolotl-graphql-yoga": "^2.1.9",
    "@ai-sdk/openai": "^1.0.0",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.2.0",
    "@tanstack/react-query": "^5.90.19",
    "ai": "^4.0.0",
    "compression": "^1.8.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "graphql": "^16.9.0",
    "graphql-yoga": "^5.10.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.12.0",
    "sirv": "^3.0.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@aexol/axolotl": "^2.1.9",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query-devtools": "^5.90.19",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.0",
    "prisma": "^7.0.0",
    "tailwindcss": "^4.1.8",
    "ts-patch": "^3.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.53.0",
    "typescript-transform-paths": "^3.5.5",
    "vite": "^6.3.5"
  },
  "peerDependencies": {}
}
